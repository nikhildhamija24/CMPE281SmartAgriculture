							 <div id="map" style="width: 1000px; height: 600px;"></div>

  <script type="text/javascript">

  var locations
   var map1 = new google.maps.Map(document.getElementById('map'), {
      zoom: 10,
      center: new google.maps.LatLng(51.57, 5.39),
      mapTypeId: google.maps.MapTypeId.ROADMAP
    });

 
    var marker =[];

	var myVar = <%- JSON.stringify(result) %>;
			var jArr=JSON.stringify(myVar);
			var result1=(JSON.parse(jArr));
			document.write(result1[0].longitude);
    for (var i = 0; i < result1.length; i++) {
  
      latLng = new google.maps.LatLng(result1[i].latitude, result1[i].longitude); 

  // Creating a marker and putting it on the map
  
 //  icons =["C:/Users/neeraj/Desktop/bullets_ball_blue_colourful_animation_clipart.GIF"]
   marker[i] = new google.maps.Marker({
    position: latLng,
    map: map1,
	description:result1[i].sensorId,
	id:i
  });
  var infoWindow = new google.maps.InfoWindow({content:"Sensor type: "+result1[i].sensorName+"  Sensor Id: "+result1[i].sensorId});
  infoWindow.open(map1,marker[i]);
  google.maps.event.addListener(marker[i], "click", function(e) {
  //alert(marker[this.id].description)
  infoWindow.open(map1,marker[this.id]);
  infoWindow.setContent(result1[i].sensorId);
  
});
 
}
  </script>
								
								
								
							
     
